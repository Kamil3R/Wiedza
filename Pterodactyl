sudo sysctl -w net.ipv4.icmp_echo_ignore_all=1   <- usuwanie pingów
sudo sysctl -w net.ipv4.icmp_echo_ignore_all=0   <- aktywacja pingów
systemctl restart wings 			 <- przeładowanie wingsów
cd /var/lib/pterodactyl/volumes/ 		 <- ściezka do serwerów
cd /var/lib/pterodactyl/volumes/

./baza.sh &					 <- odpalanie skryptu na baze w tle
chmod +x ./baza.sh 				 <- nadawanie nad uprawnień skryptowi

